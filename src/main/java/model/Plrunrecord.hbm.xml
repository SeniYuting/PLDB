<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="model.Plrunrecord" table="plrunrecord">
		<id name="id" type="int">
			<column name="id" />
			<generator class="assigned" />
		</id>
		<property name="time" type="java.sql.Date">
			<column name="time" />
		</property>
		<property name="spaceid" type="int">
			<column name="spaceid" />
		</property>
		<property name="status" type="int">
			<column name="status" />
		</property>
		<property name="step" type="int">
			<column name="step" />
		</property>
		<property name="identity" type="int">
			<column name="identity" />
		</property>
	</class>
</hibernate-mapping>